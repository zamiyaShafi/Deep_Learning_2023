   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "89c0dade",
   "metadata": {},
   "outputs": [],
   "source": [
    "while True:\n",
    "    ret, frame = cap.read()\n",
    "    cv2.imshow(\"win\", frame)\n",
    "\n",
    "    if cv2.waitKey(10) & 0xFF == ord('x'):\n",
    "        break\n",
    "\n",
    "cv2.destroyAllWindows()"
   ]
